<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="styles.css">
        <title>Homepage</title>
    </head>
    <body>

        <div class="header">
            <a href="/">SE LAB</a>
            <div class="leftOpts">
                <a href="/uploadarticle">Upload Article</a>
                <a href="/login">Login</a>
                <a href="/signup">Signup</a>
            </div>
        </div>

        

        <div class="home_container">

            

            <% for(let i=0; i<data.length; i++){%>

                <div class="box">
                    <div class="imgBox">
                        <img src=<%= data[i].imgLink %> alt="Image should be here">
                    </div>
                    <div class="content">
                        <h1><%= data[i].heading %></h1>
                        <h3><%= data[i].aboutArticle %></h3>
                        <a href=<%= data[i].articleLink %>>Read Article</a>
                    </div>
                </div>
            <% } %>

        </div>
    </body>

